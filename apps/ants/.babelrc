{
  "presets": [
    ["@babel/preset-env", {
      "bugfixes": true,
      "modules": "auto"
    }],
    ["@babel/preset-react", {
      "runtime": "automatic"
    }],
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["@babel/proposal-decorators", {
      "legacy": true
    }],
    "@babel/proposal-class-properties",
    ["import", {
      "libraryName": "@mui/material",
      "libraryDirectory": "",
      "camel2DashComponentName": false
    }, "core"],
    ["import", {
      "libraryName": "@mui/icons-material",
      "libraryDirectory": "",
      "camel2DashComponentName": false
    }, "icons"]
  ]
}
